{
    "name": "kodekeep/nova-permission",
    "description": "A Laravel Nova resource tool for spatie/laravel-permission.",
    "keywords": [
        "kodekeep",
        "nova-permission"
    ],
    "homepage": "https://github.com/kodekeep/nova-permission",
    "license": "MPL-2.0",
    "authors": [
        {
            "name": "Brian Faust",
            "email": "brian@kodekeep.com",
            "homepage": "https://kodekeep.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "illuminate/contracts": "^6.0",
        "illuminate/support": "^6.0",
        "spatie/laravel-permission": "^3.7"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "graham-campbell/analyzer": "^2.4",
        "orchestra/testbench": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "KodeKeep\\NovaPermission\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KodeKeep\\NovaPermission\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test:coverage": "vendor/bin/phpunit --coverage-html=.coverage --coverage-clover=clover.xml",
        "format": "vendor/bin/php-cs-fixer fix ./"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "KodeKeep\\NovaPermission\\ToolServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
